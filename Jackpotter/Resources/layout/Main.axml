<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <Button
        android:id="@+id/myButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/hello" />
    <Button
        android:id="@+id/pick3Button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/button_pick3" />
    <Button
        android:id="@+id/pick4Button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/button_pick4" />
    <Button
        android:id="@+id/cash5Button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/button_cash5" />
    <Button
        android:id="@+id/Luck4LifeButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/button_lucky4life" />
    <Button
        android:id="@+id/megamillsButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/button_megamillions" />
    <Button
        android:id="@+id/powerballButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/button_powerball" />
</LinearLayout>